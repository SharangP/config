## ENV

## ALIASES
# utilities
alias psg='ps aux | grep'
alias findfile='find . -type f -name'
alias makepass='openssl rand -base64 16'
function grepkill () {
    ps aux | grep $1 | awk '{print $2}' | xargs sudo kill
}

# git
alias gs='git status'
alias gd='git diff'
alias gco='git checkout'
alias gb='git branch'
alias gp='git push'
alias gpl='git pull'
alias gst='git stash'
alias gsp='git stash pop'
alias gcm='git commit -m'
alias gca='git commit --amend --no-edit'
alias gr='git rebase'
alias gm='git merge --ff-only'
alias gl='git log -n 10 --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit --date=relative'

